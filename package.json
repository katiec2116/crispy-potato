  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mysql2": "^2.2.5",
    "react-bootstrap": "^1.5.2",
    "react-router-dom": "^5.2.0",
    "sequelize": "^6.6.2"
  }
}
